{
  "env": {
    "es2020": true,
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "quotes": ["error", "double"],
    "no-console": "error",
    "semi": "error",
    "no-extra-semi": "error"
  }
}
